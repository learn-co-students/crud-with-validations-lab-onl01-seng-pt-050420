<%= form_for @song do |f| %>
  <%= f.label :title, "Song Title" %><br>
  <%= f.text_field :title %><br>
  <%= f.label :artist_name, "Artist Name" %><br>
  <%= f.text_field :artist_name %><br>
  <%= f.label :genre, "Genre Type" %><br>
  <%= f.text_field :genre %><br>
  <%= f.label :released, "Is Song Released?" %>
  <%= f.check_box :released %>
  <%= f.label :release_year, "Song Release Year"%><br>
  <%= f.text_field :release_year %><br>
  <%= f.submit "Submit" %>
<% end %>