<h2>All Songs</h2>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Artist</th>
      <th>Genre</th>
    </tr>
  </thead>
  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= link_to song.title, song_path(song) %></td>
        <td><%= song.artist_name %></td>
        <td><%= song.genre %></td>
      </tr>
    <% end %>
  <tbody>
</table>
<%= link_to "Create New Song", new_song_path %>